# GraphQL Basic enum

# GraphQL schema Type
# Caddy
type CaddyData {
  data: [Caddy!]!
  pagination: PaginationType!
}

type Caddy {
  id: String
  name: String
  location: String
  avialability: String
  skill: [String]
  star: Int
  description: String
  time: [String]
  cost: Float
  images: [String]
  courseGolfIDs: [String]
  courseGolf: [CourseGolf]
}

# GraphQl schema Input
# GetCaddyInput
input GetCaddyInput {
  language: LanguageEnum!
  id: String
  reference: String
}

# GraphQl schema Input
# CaddysHistoryInput
input GetCaddysInput {
  language: LanguageEnum!
  pagination: PaginationInput!
  skill: [String]
  courseGolfIDs: [String]
  cost: Float
  star: Int
  ids: [String]
  reference: String
}

# GraphQl schema Input
# CaddyInput
input CaddyInput {
  id: String
  reference: String
  name: String!
  location: String!
  avialability: String!
  skill: [String!]
  star: Int!
  description: String
  time: [String!]
  cost: Float!
  images: [String!]
  language: LanguageEnum!
  isActive: Boolean!
  courseGolfIDs: [String!]!
}

# Query
extend type Query {
  getCaddy(input: GetCaddyInput!): Caddy!
  getCaddys(input: GetCaddysInput!): CaddyData!
}

# Mutation
extend type Mutation {
  caddy(input: CaddyInput!): Caddy!
  deleteCaddy(id: String!): Caddy!
}
